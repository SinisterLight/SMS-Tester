<h1>Listing posts</h1>

<table>
  <tr>
    <th>Mobilenumber</th>
    <th>Smstimestamp</th>
    <th>Smstext</th>
    <th>Simnumber</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @posts.each do |post| %>
  <tr>
    <td><%= post.mobileNumber %></td>
    <td><%= post.smsTimeStamp %></td>
    <td><%= post.smsText %></td>
    <td><%= post.simNumber %></td>
    <td><%= link_to 'Show', post %></td>
    <td><%= link_to 'Edit', edit_post_path(post) %></td>
    <td><%= link_to 'Destroy', post, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Post', new_post_path %>
